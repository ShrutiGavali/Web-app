pipeline{
    agent any
    tools{
        maven 'maven_3_9_5'
    }
    stages{
        stage('Build Maven'){
            steps{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/ShrutiGavali/Web-App.git']])
                bat 'mvn clean install'
                }
            }
        }
    }
